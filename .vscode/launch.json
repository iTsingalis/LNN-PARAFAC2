    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train nnParafac2 example",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Code/train_nnParafac2.py",
            "console": "integratedTerminal",
            "args": [
                "--u_init",
                "uniform",
                "--w_init",
                "nndsvdar",
                "--minR",
                "1500",
                "--maxR",
                "4500",
                "--stepR",
                "500",
                "--target_lang",
                "spanish",
                "--root",
                "/media/red/tsingalis/gitRepositories/LNN-PARAFAC2/",
                "--tokenizer",
                "moses",
                "--ngram_range_max",
                "1",
                "--ngram_range_min",
                "1",
                "--alg",
                "nnParafac2",
                "--analyzer",
                "word",
                "--parallel_dataset",
                "WikiMatrix",
                "--parallel_n_samples",
                "100000"
            ],
            "env": {
                "PYTHONPATH": "/media/red/tsingalis/gitRepositories/LNN-PARAFAC2"
            }
        },
        {
            "name": "Test nnParafac2 example",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Code/eval_nnParafac2.py",
            "console": "integratedTerminal",
            "args": [
                "--alg",
                "nnParafac2",
                "--clf",
                "LogisticRegression",
                "--sub_clf_name",
                "LogisticRegression",
                "--target_task",
                "MlDoc",
                "--target_lang",
                "spanish",
                "--masked",
                "--root",
                "/media/red/tsingalis/gitRepositories/LNN-PARAFAC2/",
                "--loaded_run",
                "1",
                "--loaded_rank",
                "3000",
                "--parallel_dataset",
                "WikiMatrix"
            ],
            "env": {
                "PYTHONPATH": "/media/red/tsingalis/gitRepositories/LNN-PARAFAC2"
            }
        }
    ]
}